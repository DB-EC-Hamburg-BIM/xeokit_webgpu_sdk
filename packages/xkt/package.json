{
  "name": "@xeokit/xkt",
  "type": "module",
  "version": "0.0.1",
  "description": "Import and export models as xeokit's native binary XKT format",
  "main": "dist/index.js",
  "module": "dist/index.es",
  "source": "src/index.ts",
  "scripts": {
    "clean": "rimraf dist && mkdirp dist",
    "publish": "npm publish --access public",
    "dist": "rimraf dist && mkdirp dist && microbundle --no-compress --define PACKAGE_VERSION=$npm_package_version",
    "watch": "microbundle --no-compress watch --define PACKAGE_VERSION=$npm_package_version",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xeokit/sdk.git"
  },
  "author": "Lindsay Kay",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/xeokit/sdk/issues"
  },
  "homepage": "https://xeokit.github.io/sdk/docs/modules/_xeokit_xkt.html",
  "files": [
    "dist/",
    "src/",
    "README.md",
    "package.json",
    "package-lock.json"
  ],
  "keywords": [],
  "dependencies": {
    "@xeokit/core": "^0.0.1",
    "@xeokit/scene": "^0.0.1",
    "@xeokit/data": "^0.0.1",
    "@xeokit/math": "^0.0.1",
    "pako": "^2.0.4"
  },
  "devDependencies": {
    "@swc/core": "^1.3.26",
    "@swc/jest": "^0.2.24",
    "@types/jest": "^29.2.5",
    "@types/pako": "^2.0.0",
    "jest": "^29.3.1",
    "typescript": "^4.9.4"
  }
}
