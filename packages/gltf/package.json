{
  "name": "@xeokit/gltf",
  "type": "module",
  "version": "0.0.1",
  "description": "glTF loader for xeokit",
  "main": "dist/index.js",
  "module": "dist/index.es",
  "source": "src/index.ts",
  "scripts": {
    "clean": "rm -fr dist",
    "publish": "npm publish --access public",
    "dist": "rm -fr dist && microbundle --no-sourcemap  --no-compress --define PACKAGE_VERSION=$npm_package_version",
    "watch": "microbundle --no-sourcemap   --no-compress watch --define PACKAGE_VERSION=$npm_package_version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xeokit/sdk.git"
  },
  "author": "Lindsay Kay",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/xeokit/sdk/issues"
  },
  "homepage": "https://xeokit.io",
  "files": [
    "src/",
    "README.md",
    "package.json",
    "package-lock.json"
  ],
  "keywords": [

  ],
  "dependencies": {
    "@xeokit/constants": "^0.0.1",
    "@xeokit/core": "^0.0.1",
    "@xeokit/matrix": "^0.0.1",
    "@xeokit/scene": "^0.0.1",
    "@xeokit/math": "^0.0.1",
    "@xeokit/data": "^0.0.1",
    "@jest/globals": "^29.5.0",
    "@loaders.gl/core": "^3.2.6",
    "@loaders.gl/gltf": "^3.2.6",
    "@loaders.gl/polyfills": "^3.2.6",
    "@loaders.gl/textures": "^3.2.6",
    "core-js": "^3.22.5",
    "fs": "0.0.1-security",
    "path": "^0.12.7"
  }
}
