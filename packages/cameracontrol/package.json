{
  "name": "@xeokit/cameracontrol",
  "type": "module",
  "version": "0.0.1",
  "description": "Camera navigation controls for a xeokit Viewer",
  "main": "dist/index.js",
  "module": "dist/index.es",
  "source": "src/index.ts",
  "scripts": {
    "clean": "rimraf dist && mkdirp dist",
    "dist": "rimraf dist && mkdirp dist && microbundle --no-sourcemap  --no-compress --define PACKAGE_VERSION=$npm_package_version",
    "watch": "microbundle --no-sourcemap watch --define PACKAGE_VERSION=$npm_package_version",
    "publish": "npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xeokit/sdk.git"
  },
  "author": "Lindsay Kay",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/xeokit/sdk/issues"
  },
  "homepage": "https://xeokit.github.io/sdk/docs/modules/_xeokit_cameracontrol.html",
  "files": [
    "src/",
    "dist/",
    "README.md",
    "package.json",
    "package-lock.json"
  ],
  "keywords": [],
  "dependencies": {
    "strongly-typed-events": "3.0.7",
    "@xeokit/core": "0.0.1",
    "@xeokit/math": "0.0.1",
    "@xeokit/matrix": "0.0.1",
    "@xeokit/viewer": "0.0.1"
  },
  "devDependencies": {
  "typescript": "^4.9.4"
  }
}
