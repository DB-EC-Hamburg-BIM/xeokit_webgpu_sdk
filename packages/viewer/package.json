{
  "name": "@xeokit/viewer",
  "version": "0.0.1-alpha-1",
  "description": "An extensible browser-based 3D viewer for AECO applications",
  "module": "dist/xeokit-viewer.es.js",
  "source": "src/index.ts",
  "scripts": {
    "dist": "microbundle --format modern,cjs --define PACKAGE_VERSION=$npm_package_version",
    "build": "rm -rf dist && cross-env NODE_ENV=production PLUGIN=xkt INPUT=./index.js rollup -c ../../rollup/rollup.viewer.config.js",
    "publish": "npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xeokit/xeokit-viewer.git"
  },
  "author": "Lindsay Kay",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/xeokit/xeokit-viewer/issues"
  },
  "homepage": "https://xeokit.io",
  "files": [
    "dist/",
    "src/",
    "INDEX.md",
    "package.json",
    "package-lock.json"
  ],
  "keywords": [
    "webgl",
    "webgpu",
    "javascript",
    "xeokit",
    "xeolabs",
    "creoox",
    "ifc",
    "bcf",
    "bim",
    "gis",
    "cad",
    "openbim",
    "opensource",
    "gltf",
    "3dxml",
    "obj",
    "stl",
    "las",
    "pointcloud",
    "3d-viewer"
  ],
  "main": "dist/xeokit-viewer.es.js"
}
