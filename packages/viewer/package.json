{
  "name": "@xeokit/viewer",
  "type": "module",
  "version": "0.0.1",
  "description": "An extensible browser-based 3D/2D model viewer for AECO applications",
  "main": "dist/index.js",
  "module": "dist/index.es",
  "source": "src/index.ts",
  "scripts": {
    "clean": "rimraf dist && mkdirp dist",
    "publish": "npm publish --access public",
    "dist": "rimraf dist && mkdirp dist && microbundle --no-sourcemap  --no-compress --define PACKAGE_VERSION=$npm_package_version",
    "watch": "microbundle --no-sourcemap   --no-compress watch --define PACKAGE_VERSION=$npm_package_version",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xeokit/sdk.git"
  },
  "author": "Lindsay Kay",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/xeokit/sdk/issues"
  },
  "homepage": "https://xeokit.github.io/sdk/docs/modules/_xeokit_viewer.html",
  "files": [
    "src/",
    "dist/",
    "README.md",
    "package.json",
    "package-lock.json"
  ],
  "keywords": [
  ],
  "dependencies": {
    "@xeokit/core": "^0.0.1",
    "@xeokit/constants": "^0.0.1",
    "@xeokit/scene": "^0.0.1",
    "@xeokit/math": "^0.0.1",
    "@xeokit/boundaries": "^0.0.1",
    "@xeokit/matrix": "^0.0.1",
    "@xeokit/utils": "^0.0.1",
    "strongly-typed-events": "3.0.7"
  }
}
